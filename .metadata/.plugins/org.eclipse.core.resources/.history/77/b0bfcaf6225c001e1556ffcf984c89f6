package com.kjs.service;

import javax.inject.Inject;

import org.springframework.stereotype.Service;

import com.kjs.dao.TestDAO;

@Service
public class TestService {
	
	@Inject
	private TestDAO dao;
	
	public String testService(String testValue) {
		System.out.println("service");
		String result = "";
		
		if (dao.testInsert(testValue) == 1) {
			result = "insert 성공";
		} else {
			result = "insert 실패";
		}
		
		return result;
	}
}